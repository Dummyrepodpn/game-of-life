node('new_node') {
    stage('vcn') {
        git url: 'https://github.com/Dummyrepodpn/game-of-life.git',
            branch: 'scripted',
            poll: false
    }
    stage('build') {
        sh 'export PATH="/usr/lib/jvm/java-1.8.0-openjdk-amd64/bin:$PATH" && mvn package'
    }
    stage('artifacts') {
        archiveArtifacts artifacts: '**/gameoflife.war'
    }
    stage('test results') {
        junit testResults: '**/TEST-*.xml'
    }
}